<Window x:Class="WpfUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnCustomer" Content="Customers" HorizontalAlignment="Left" Margin="90,0,0,0" VerticalAlignment="Top" Width="80" RenderTransformOrigin="0.483,1.989" Height="45" Click="btnCustomer_Click"/>
        <Button x:Name="btnMain" Content="Main Menu" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="80" RenderTransformOrigin="0.483,1.989" Height="45"/>

    </Grid>
</Window>
